{
    "extends": ["react-app", "prettier"],
"plugins": ["react", "prettier"],
"rules": {
  "prettier/prettier": [
    "warn",
    {
      "arrowParens": "avoid",
      "semi": true,
      "trailingComma": "none",
      "endOfLine": "lf",
      "tabWidth": 2,
      "useTabs": false,
      "singleQuote": true,
      "printWidth": 120,
      "jsxSingleQuote": false
    }
  ]
}
}
